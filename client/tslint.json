{
  "extends": ["tslint:recommended", "tslint-config-standard", "tslint-config-prettier"],
  "rules": {
    "member-access": false,
    "interface-over-type-literal": false,
    "member-ordering": [false],
    "object-literal-sort-keys": false,
    "interface-name": false
  },
  "defaultSeverity": "warning"
}
